    "express": "^4.17.1",
    "graphql-tag": "^2.12.5",
    "node-fetch": "^2.6.1",
    "parse": "^3.2.0",
    "parse-dashboard": "^2.1.0",
    "parse-server": "^4.5.0",
    "web3": "^1.4.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "1.21.6",
    "@types/express": "^4.17.12",
    "@types/parse": "^2.18.6",
    "@types/web3": "^1.2.2",
    "ts-proto": "^1.82.0",
    "typescript": "^4.3.5",
    "@graphql-codegen/typescript": "1.22.4",
